{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "script"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "max-len": 0,
    "no-continue": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "prefer-rest-params": 0,
    "arrow-body-style": 0
  }
}
